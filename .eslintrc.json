{
  "plugins": ["prettier", "import", "@typescript-eslint"],
  "parser": "@typescript-eslint/parser",
  "extends": [
    "plugin:@typescript-eslint/recommended",
    "prettier",
    "airbnb",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "import/extensions": [
    "error",
    "ignorePackages",
    {
      "ts": "never"
    }
  ],
  "prettier/prettier": ["error", { "endOfLine": "off" }]
}
